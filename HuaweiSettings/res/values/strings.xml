    <!-- RIL -->
    <string name="pref_huawei_simsetting_title">Dual SIM</string>
    <string name="pref_huawei_simsetting_description">Switch the device from single-sim to dual-sim. Use this switch ONLY if you have two SIMs inside your device. (Requires reboot)</string>
    <string name="pref_huawei_simsetting_key" translatable="false">pref_huawei_simsetting</string>

    <!-- Gestures -->
    <string name="pref_huawei_dt2w_title">Double tap to wake</string>
    <string name="pref_huawei_dt2w_description"></string>
    <string name="pref_huawei_dt2w_key" translatable="false">pref_huawei_dt2w</string>

    <!-- Workarounds -->
    <string name="pref_aospa_vsync_title">Software vsync</string>
    <string name="pref_aospa_vsync_description">Disable hwcomposer vsync to fix lagging issue. (Requires reboot)</string>
    <string name="pref_aospa_vsync_key" translatable="false">pref_aospa_vsync</string>

    <string name="pref_noble_title">Bluetooth pairing</string>
    <string name="pref_noble_description">Fixes bluetooth pairing with the side effect of disabling BLE devices. (Requires reboot)</string>
    <string name="pref_noble_key" translatable="false">pref_noble</string>

    <!-- HAL -->
    <string name="pref_hal_power_title">Stock power</string>
    <string name="pref_hal_power_description">Disables open-source power HAL. (Requires reboot)</string>
    <string name="pref_hal_power_key" translatable="false">pref_hal_power</string>

    <string name="pref_hal_lights_title">Stock lights</string>
    <string name="pref_hal_lights_description">Disables open-source lights HAL. (Requires reboot)</string>
    <string name="pref_hal_lights_key" translatable="false">pref_hal_lights</string>

    <string name="pref_hal_sensors_title">Emulated gyroscope</string>
    <string name="pref_hal_sensors_description">Enables experimental gyroscope sensor. (Requires reboot)</string>
    <string name="pref_hal_sensors_key" translatable="false">pref_hal_sensors</string>

    <!-- Media -->
    <string name="pref_media_encoder_title">Google AVC encoder</string>
    <string name="pref_media_encoder_description">Replaces stock video encoder with the Google one. May fix camera in some apps. (Requires reboot)</string>
    <string name="pref_media_encoder_key" translatable="false">pref_media_encoder</string>

    <string name="pref_media_albumart_title">Lockscreen album art</string>
    <string name="pref_media_albumart_description">Disables the album art on lockscreen. Usable for Spotify, which doesn\'t have an option for it.</string>
    <string name="pref_media_albumart_key" translatable="false">pref_media_albumart</string>

